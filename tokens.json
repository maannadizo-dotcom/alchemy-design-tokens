{
  "$schema": "https://schemas.tokens.studio/v1",
  "$description": "Alchemy tokens (single-file) for Tokens Studio single-file sync",
  "color": {
    "neutral": {
      "light": {
        "$type": "color",
        "$value": "#F7F9FA"
      },
      "mid": {
        "$type": "color",
        "$value": "#E0E4E6"
      },
      "dark": {
        "$type": "color",
        "$value": "#101820"
      }
    },
    "accent": {
      "primary": {
        "$type": "color",
        "$value": "#CBB5E8"
      },
      "secondary": {
        "$type": "color",
        "$value": "#9FBEE5"
      }
    }
  },
  "typography": {
    "hero": {
      "$type": "typography",
      "$value": {
        "fontFamily": "Satoshi",
        "fontWeight": 600,
        "fontSize": "56px",
        "lineHeight": "110%",
        "letterSpacing": "-0.5%"
      }
    },
    "heading": {
      "$type": "typography",
      "$value": {
        "fontFamily": "Inter",
        "fontWeight": 500,
        "fontSize": "32px",
        "lineHeight": "130%"
      }
    },
    "body": {
      "$type": "typography",
      "$value": {
        "fontFamily": "Inter",
        "fontWeight": 400,
        "fontSize": "16px",
        "lineHeight": "160%"
      }
    },
    "microcopy": {
      "$type": "typography",
      "$value": {
        "fontFamily": "Inter",
        "fontWeight": 400,
        "fontSize": "13px",
        "lineHeight": "150%"
      }
    }
  },
  "radius": {
    "base": {
      "$type": "borderRadius",
      "$value": "12px"
    },
    "card": {
      "$type": "borderRadius",
      "$value": "16px"
    },
    "button": {
      "$type": "borderRadius",
      "$value": "8px"
    }
  },
  "spacing": {
    "unit": {
      "$type": "spacing",
      "$value": "8px"
    },
    "xs": {
      "$type": "spacing",
      "$value": "4px"
    },
    "sm": {
      "$type": "spacing",
      "$value": "8px"
    },
    "md": {
      "$type": "spacing",
      "$value": "16px"
    },
    "lg": {
      "$type": "spacing",
      "$value": "24px"
    },
    "xl": {
      "$type": "spacing",
      "$value": "40px"
    }
  },
  "shadow": {
    "soft": {
      "$type": "boxShadow",
      "$value": {
        "x": 0,
        "y": 8,
        "blur": 24,
        "spread": 0,
        "color": "rgba(0,0,0,0.04)"
      }
    },
    "medium": {
      "$type": "boxShadow",
      "$value": {
        "x": 0,
        "y": 12,
        "blur": 32,
        "spread": 0,
        "color": "rgba(0,0,0,0.08)"
      }
    }
  },
  "component": {
    "button": {
      "primary": {
        "$type": "composition",
        "$value": {
          "fill": "{color.accent.primary}",
          "textColor": "{color.neutral.dark}",
          "radius": "{radius.button}",
          "paddingY": "{spacing.sm}",
          "paddingX": "{spacing.md}",
          "shadow": "{shadow.soft}"
        }
      },
      "accent": {
        "$type": "composition",
        "$value": {
          "fill": "{color.accent.secondary}",
          "textColor": "{color.neutral.dark}",
          "radius": "{radius.button}",
          "paddingY": "{spacing.sm}",
          "paddingX": "{spacing.md}",
          "shadow": "{shadow.medium}"
        }
      }
    },
    "card": {
      "base": {
        "$type": "composition",
        "$value": {
          "fill": "{color.neutral.light}",
          "textColor": "{color.neutral.dark}",
          "radius": "{radius.card}",
          "shadow": "{shadow.soft}",
          "padding": "{spacing.lg}"
        }
      }
    },
    "input": {
      "field": {
        "$type": "composition",
        "$value": {
          "fill": "{color.neutral.light}",
          "textColor": "{color.neutral.dark}",
          "radius": "{radius.base}",
          "paddingY": "{spacing.sm}",
          "paddingX": "{spacing.md}",
          "borderColor": "{color.neutral.mid}"
        }
      },
      "focus": {
        "$type": "composition",
        "$value": {
          "borderColor": "{color.accent.primary}",
          "shadow": "{shadow.soft}"
        }
      }
    },
    "typography": {
      "hero": {
        "$type": "typography",
        "$value": "{typography.hero}"
      },
      "heading": {
        "$type": "typography",
        "$value": "{typography.heading}"
      },
      "body": {
        "$type": "typography",
        "$value": "{typography.body}"
      },
      "microcopy": {
        "$type": "typography",
        "$value": "{typography.microcopy}"
      }
    }
  },
  "motion": {
    "duration": {
      "base": {
        "$type": "duration",
        "$value": "200ms"
      },
      "page": {
        "$type": "duration",
        "$value": "400ms"
      },
      "emotionShift": {
        "$type": "duration",
        "$value": "600ms"
      }
    },
    "easing": {
      "easeOut": {
        "$type": "cubicBezier",
        "$value": [
          0.25,
          0.1,
          0.25,
          1
        ]
      },
      "easeInOut": {
        "$type": "cubicBezier",
        "$value": [
          0.42,
          0,
          0.58,
          1
        ]
      },
      "breath": {
        "$type": "cubicBezier",
        "$value": [
          0.2,
          0.8,
          0.3,
          1
        ]
      }
    },
    "preset": {
      "enterFade": {
        "$type": "composition",
        "$value": {
          "duration": "{motion.duration.base}",
          "easing": "{motion.easing.easeOut}",
          "opacityFrom": 0,
          "opacityTo": 1
        }
      },
      "pageTransition": {
        "$type": "composition",
        "$value": {
          "duration": "{motion.duration.page}",
          "easing": "{motion.easing.easeInOut}",
          "type": "opacity+position"
        }
      },
      "emotionShift": {
        "$type": "composition",
        "$value": {
          "duration": "{motion.duration.emotionShift}",
          "easing": "{motion.easing.breath}",
          "type": "crossfade+color"
        }
      }
    }
  },
  "$themes": [
    {
      "id": "clarity",
      "selectedTokenSets": {}
    },
    {
      "id": "focus",
      "selectedTokenSets": {}
    },
    {
      "id": "reflection",
      "selectedTokenSets": {}
    },
    {
      "id": "resolve",
      "selectedTokenSets": {}
    },
    {
      "id": "transformation",
      "selectedTokenSets": {}
    }
  ],
  "emotion": {
    "clarity": {
      "color": {
        "background": {
          "$type": "color",
          "$value": "#C5D7E5"
        },
        "surface": {
          "$type": "color",
          "$value": "{color.neutral.light}"
        },
        "text": {
          "$type": "color",
          "$value": "{color.neutral.dark}"
        },
        "primary": {
          "$type": "color",
          "$value": "#C5D7E5"
        },
        "accent": {
          "$type": "color",
          "$value": "{color.accent.secondary}"
        }
      }
    },
    "focus": {
      "color": {
        "background": {
          "$type": "color",
          "$value": "#9FBEE5"
        },
        "surface": {
          "$type": "color",
          "$value": "{color.neutral.light}"
        },
        "text": {
          "$type": "color",
          "$value": "{color.neutral.dark}"
        },
        "primary": {
          "$type": "color",
          "$value": "#9FBEE5"
        },
        "accent": {
          "$type": "color",
          "$value": "{color.accent.secondary}"
        }
      }
    },
    "reflection": {
      "color": {
        "background": {
          "$type": "color",
          "$value": "#A9B8BA"
        },
        "surface": {
          "$type": "color",
          "$value": "{color.neutral.light}"
        },
        "text": {
          "$type": "color",
          "$value": "{color.neutral.dark}"
        },
        "primary": {
          "$type": "color",
          "$value": "#A9B8BA"
        },
        "accent": {
          "$type": "color",
          "$value": "{color.accent.primary}"
        }
      }
    },
    "resolve": {
      "color": {
        "background": {
          "$type": "color",
          "$value": "#CBB5E8"
        },
        "surface": {
          "$type": "color",
          "$value": "{color.neutral.light}"
        },
        "text": {
          "$type": "color",
          "$value": "{color.neutral.dark}"
        },
        "primary": {
          "$type": "color",
          "$value": "#CBB5E8"
        },
        "accent": {
          "$type": "color",
          "$value": "{color.accent.secondary}"
        }
      }
    },
    "transformation": {
      "color": {
        "background": {
          "$type": "color",
          "$value": "{emotion.resolve.color.background}"
        },
        "surface": {
          "$type": "color",
          "$value": "{color.neutral.light}"
        },
        "text": {
          "$type": "color",
          "$value": "{color.neutral.dark}"
        }
      }
    }
  }
}